  "bugs": {
    "url": "https://github.com/BrightspaceUI/date-picker/issues"
  },
  "homepage": "https://github.com/BrightspaceUI/date-picker#readme",
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-brightspace": "^0.2.4",
    "eslint-plugin-html": "^2.0.3"
  }
}
